{
  "compilerOptions": {
    "jsx": "react-native",
    "allowJs": true,
    "noEmit": true,
    "isolatedModules": true,
    "baseUrl": ".",  
    "paths": {
      "@/*": ["app/*"],
      "#/*": ["./*"]
    }
  },
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base",
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "app/index.jsx", "app/_layout.tsx", "app/(auth)/_layout.jsx"  ]
}
